<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--Rotate3dAnimation-->
    <declare-styleable name="Rotate3dAnimation">
        <attr name="rollType" format="enum">
            <enum name="x" value="0" />
            <enum name="y" value="1" />
            <enum name="z" value="2" />
        </attr>
        <attr name="fromDeg" format="float" />
        <attr name="toDeg" format="float" />
        <attr name="valuePivotX" format="fraction" />
        <attr name="valuePivotY" format="fraction" />
    </declare-styleable>
    <!--WheelView-->
    <declare-styleable name="WheelView">
        <attr name="wheelViewGravity">
            <enum name="center" value="17" />
            <enum name="left" value="3" />
            <enum name="right" value="5" />
        </attr>
        <attr name="wheelViewTextSize" format="dimension" />
        <attr name="wheelViewTextColorOut" format="color" />
        <attr name="wheelViewTextColorCenter" format="color" />
        <attr name="wheelViewDividerColor" format="color" />
        <attr name="wheelViewLineSpacingMultiplier" format="float" />
    </declare-styleable>
    <!--LowerConcaveView-->
    <declare-styleable name="LowerConcaveView">
        <attr name="lowerConcaveViewArcHeight" format="dimension" />
    </declare-styleable>
    <!--LowerConvexView-->
    <declare-styleable name="LowerConvexView">
        <attr name="lowerConvexViewArcHeight" format="dimension" />
        <attr name="lowerConvexViewBackgroundColor" format="color" />
    </declare-styleable>
    <!--SearchView-->
    <declare-styleable name="SearchView">
        <attr name="textSizeSearch" format="dimension" />
        <attr name="textColorSearch" format="color" />
        <attr name="textHintSearch" format="string" />
        <attr name="searchBlockHeight" format="integer" />
        <attr name="searchBlockColor" format="color" />
    </declare-styleable>
    <!--JellyToolbar-->
    <declare-styleable name="JellyToolbar">
        <attr name="title" format="string" />
        <attr name="titleTextColor" format="color" />
        <attr name="icon" format="reference" />
        <attr name="cancelIcon" format="reference" />
        <attr name="startColor" format="color" />
        <attr name="endColor" format="color" />
    </declare-styleable>
    <!--FillTextView-->
    <declare-styleable name="FillTextView">
        <attr name="filledText" format="string" />
        <attr name="fillTextSize" format="dimension" />
        <attr name="normalColor" format="color" />
        <attr name="fillColor" format="color" />
        <attr name="rowSpace" format="dimension" />
    </declare-styleable>
    <!--CounterFloatingActionButton-->
    <declare-styleable name="CounterFloatingActionButton">
        <attr name="badgeTextColor" format="color" />
        <attr name="badgeBackgroundColor" format="color" />
        <attr name="badgePosition" format="enum">
            <enum name="RightTop" value="0" />
            <enum name="LeftBottom" value="1" />
            <enum name="LeftTop" value="2" />
            <enum name="RightBottom" value="3" />
        </attr>
    </declare-styleable>
    <!--MultiProgressBar-->
    <declare-styleable name="MultiProgressBar">
        <!--样式（默system）-->
        <attr name="multiProgressBarMode" format="enum">
            <!--系统-->
            <enum name="system" value="0" />
            <!--水平-->
            <enum name="horizontal" value="1" />
            <!--圆-->
            <enum name="circle" value="2" />
            <!--彗星-->
            <enum name="comet" value="3" />
            <!--波浪-->
            <enum name="wave" value="4" />
        </attr>
        <!--文本色（默#70A800）-->
        <attr name="multiProgressBarTextColor" format="reference|color" />
        <!--文本大小（默10sp）-->
        <attr name="multiProgressBarTextSize" format="dimension" />
        <!--文本同进度条间距（默4dp）-->
        <attr name="multiProgressBarTextMargin" format="dimension" />
        <!--已完成进度色（默#70A800）-->
        <attr name="multiProgressBarReachedColor" format="reference|color" />
        <!--已完成进度高（默2dp）-->
        <attr name="multiProgressBarReachedHeight" format="dimension" />
        <!--未完成进度色（默#CCCCCC）-->
        <attr name="multiProgressBarUnReachedColor" format="reference|color" />
        <!--未完成进度高（默1dp）-->
        <attr name="multiProgressBarUnReachedHeight" format="dimension" />
        <!--圆角否（默false）-->
        <attr name="multiProgressBarIsCapRounded" format="boolean" />
        <!--隐文本否（默false）-->
        <attr name="multiProgressBarIsHiddenText" format="boolean" />
        <!--圆进度条半径（默16dp）-->
        <attr name="multiProgressBarRadius" format="dimension" />
    </declare-styleable>
    <!--ProgressWheel-->
    <declare-styleable name="ProgressWheel">
        <attr name="ProgressWheelProgressIndeterminate" format="boolean" />
        <attr name="ProgressWheelBarColor" format="color" />
        <attr name="ProgressWheelRimColor" format="color" />
        <attr name="ProgressWheelRimWidth" format="dimension" />
        <attr name="ProgressWheelSpinSpeed" format="float" />
        <attr name="ProgressWheelBarSpinCycleTime" format="integer" />
        <attr name="ProgressWheelCircleRadius" format="dimension" />
        <attr name="ProgressWheelFillRadius" format="boolean" />
        <attr name="ProgressWheelBarWidth" format="dimension" />
        <attr name="ProgressWheelLinearProgress" format="boolean" />
    </declare-styleable>
    <!--PercentLayout-->
    <declare-styleable name="PercentLayout">
        <attr name="layout_widthPercent" format="string" />
        <attr name="layout_heightPercent" format="string" />
        <attr name="layout_marginPercent" format="string" />
        <attr name="layout_marginLeftPercent" format="string" />
        <attr name="layout_marginTopPercent" format="string" />
        <attr name="layout_marginRightPercent" format="string" />
        <attr name="layout_marginBottomPercent" format="string" />
        <attr name="layout_marginStartPercent" format="string" />
        <attr name="layout_marginEndPercent" format="string" />
        <attr name="layout_textSizePercent" format="string" />
        <attr name="layout_maxWidthPercent" format="string" />
        <attr name="layout_maxHeightPercent" format="string" />
        <attr name="layout_minWidthPercent" format="string" />
        <attr name="layout_minHeightPercent" format="string" />
        <attr name="layout_paddingPercent" format="string" />
        <attr name="layout_paddingTopPercent" format="string" />
        <attr name="layout_paddingBottomPercent" format="string" />
        <attr name="layout_paddingLeftPercent" format="string" />
        <attr name="layout_paddingRightPercent" format="string" />
    </declare-styleable>
    <!--RippleView-->
    <declare-styleable name="RippleLayout">
        <attr name="RippleLayoutAlpha" format="integer" />
        <attr name="RippleLayoutFrameRate" format="integer" />
        <attr name="RippleLayoutRippleDuration" format="integer" />
        <attr name="RippleLayoutZoomDuration" format="integer" />
        <attr name="RippleLayoutColor" format="color" />
        <attr name="RippleLayoutCentered" format="boolean" />
        <attr name="RippleLayoutType" format="enum">
            <enum name="simpleRipple" value="0" />
            <enum name="doubleRipple" value="1" />
            <enum name="rectangle" value="2" />
        </attr>
        <attr name="RippleLayoutRipplePadding" format="dimension" />
        <attr name="RippleLayoutZoom" format="boolean" />
        <attr name="RippleLayoutZoomScale" format="float" />
    </declare-styleable>
    <!--CollapsibleCard-->
    <declare-styleable name="CollapsibleCard">
        <attr name="cardTitle" format="string" />
        <attr name="cardDescription" format="string" />
    </declare-styleable>
    <!--WaveSideBar-->
    <declare-styleable name="WaveSideBar">
        <attr name="textColor" format="color|reference" />
        <attr name="backgroundColor" format="color|reference" />
        <attr name="chooseTextColor" format="color|reference" />
        <attr name="textSize" format="dimension" />
        <attr name="hintTextSize" format="dimension" />
        <attr name="radius" format="dimension" />
        <attr name="circleRadius" format="dimension" />
    </declare-styleable>
    <!--WaterfallToolbar-->
    <declare-styleable name="WaterfallToolbar">
        <attr name="initial_elevation" format="dimension" />
        <attr name="final_elevation" format="dimension" />
        <attr name="scroll_final_elevation" format="integer" />
    </declare-styleable>
    <!--MultiSearchView-->
    <declare-styleable name="MultiSearchView">
        <attr name="searchTextStyle" format="reference" />
    </declare-styleable>
    <!--ProgressLayout-->
    <declare-styleable name="ProgressLayout">
        <attr name="emptyColor" format="color" />
        <attr name="loadedColor" format="color" />
        <attr name="maxProgress" format="integer" />
        <attr name="autoProgress" format="boolean" />
    </declare-styleable>
    <!--ConnectionStatusView-->
    <declare-styleable name="ConnectionStatusView">
        <attr name="complete" format="reference" />
        <attr name="error" format="reference" />
        <attr name="loading" format="reference" />
    </declare-styleable>
    <!--CamberContainer-->
    <declare-styleable name="CamberContainer">
        <attr name="CamberContainerCurvature" format="dimension" />
        <attr name="CamberContainerGravity" format="integer">
            <flag name="top" value="0" />
            <flag name="bottom" value="1" />
        </attr>
        <attr name="CamberContainerCurvatureDirection" format="integer">
            <flag name="outward" value="0" />
            <flag name="inward" value="1" />
        </attr>
        <attr name="CamberContainerCamberTintMode" format="integer">
            <flag name="automatic" value="0" />
            <flag name="manual" value="1" />
        </attr>
        <attr name="CamberContainerCamberTintColor" format="color" />
        <attr name="CamberContainerCamberTintAlpha" format="integer" />
        <attr name="CamberContainerGradientDirection" format="integer">
            <flag name="top_to_bottom" value="0" />
            <flag name="bottom_to_top" value="1" />
            <flag name="left_to_right" value="2" />
            <flag name="right_to_left" value="3" />
        </attr>
        <attr name="CamberContainerGradientStartColor" format="color" />
        <attr name="CamberContainerGradientEndColor" format="color" />
    </declare-styleable>
    <!--CamberImageView-->
    <declare-styleable name="CamberImageView">
        <attr name="CamberImageViewCurvature" format="dimension" />
        <attr name="CamberImageViewGravity" format="integer">
            <flag name="top" value="0" />
            <flag name="bottom" value="1" />
        </attr>
        <attr name="CamberImageViewCurvatureDirection" format="integer">
            <flag name="outward" value="0" />
            <flag name="inward" value="1" />
        </attr>
        <attr name="CamberImageViewCamberTintMode" format="integer">
            <flag name="automatic" value="0" />
            <flag name="manual" value="1" />
        </attr>
        <attr name="CamberImageViewCamberTintColor" format="color" />
        <attr name="CamberImageViewCamberTintAlpha" format="integer" />
        <attr name="CamberImageViewGradientDirection" format="integer">
            <flag name="top_to_bottom" value="0" />
            <flag name="bottom_to_top" value="1" />
            <flag name="left_to_right" value="2" />
            <flag name="right_to_left" value="3" />
        </attr>
        <attr name="CamberImageViewGradientStartColor" format="color" />
        <attr name="CamberImageViewGradientEndColor" format="color" />
    </declare-styleable>
    <!--ElasticCardView-->
    <declare-styleable name="ElasticCardView">
        <attr name="flexibility" format="float" />
    </declare-styleable>
    <!--TipView-->
    <declare-styleable name="TipView">
        <attr name="tipTitle" format="string" />
        <attr name="tipMessage" format="string" />
        <attr name="tipTextColor" format="color" />
        <attr name="anchoredView" format="reference" />
        <attr name="cornerRadius" format="dimension" />
        <attr name="tipBackgroundColor" format="color" />
        <attr name="arrowHeight" format="dimension" />
        <attr name="arrowWidth" format="dimension" />
        <attr name="closeButtonVisibility" format="enum">
            <enum name="visible" value="0" />
            <enum name="gone" value="1" />
        </attr>
        <attr name="closeButtonGravity" format="integer">
            <enum name="top" value="0" />
            <enum name="center" value="1" />
            <enum name="bottom" value="2" />
        </attr>
        <attr name="arrowLocation" format="integer">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
        </attr>
    </declare-styleable>
    <!--TagView-->
    <declare-styleable name="TagView">
        <attr name="lineMargin" format="dimension" />
        <attr name="tagMargin" format="dimension" />
        <attr name="textPaddingLeft" format="dimension" />
        <attr name="textPaddingRight" format="dimension" />
        <attr name="textPaddingTop" format="dimension" />
        <attr name="textPaddingBottom" format="dimension" />
    </declare-styleable>
</resources>