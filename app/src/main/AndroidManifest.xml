        <activity android:name="example.onepartylibrary.card.elastic.ElasticCardViewActivity" />
        <activity android:name="example.onepartylibrary.card.CardActivity" />
        <activity android:name="example.onepartylibrary.layout.CircularRevealActivity" />
        <activity android:name="example.onepartylibrary.layout.ProgressLayoutActivity" />
        <activity android:name="example.onepartylibrary.layout.RippleLayoutActivity" />
        <activity android:name="example.onepartylibrary.chart.RadarChartActivity" />
