        <activity android:name="example.onepartylibrary.clock.ClockActivity" />
        <activity android:name="example.onepartylibrary.picture.EasingActivity" />
        <activity android:name="example.onepartylibrary.layout.CamberLayoutActivity" />
        <activity android:name="example.onepartylibrary.connection.ConnectionActivity" />
        <activity android:name="example.onepartylibrary.toolbar.ToolbarActivity" />
        <activity android:name="example.onepartylibrary.guide.GuideTwoActivity" />
