<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToPadding="false"
    android:orientation="vertical"
    android:padding="@dimen/dp_12"
    tools:context=".MainActivity">

    <com.google.android.material.button.MaterialButton
        android:id="@+id/mainActivityMbPicture"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/picture"
        android:textAppearance="?android:attr/textAppearanceInverse" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/mainActivitySearchBoxOne"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/searchBoxOne"
        android:textAppearance="?android:attr/textAppearanceInverse" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/mainActivitySearchBoxTwo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/searchBoxTwo"
        android:textAppearance="?android:attr/textAppearanceInverse" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/mainActivityTimer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/timer"
        android:textAppearance="?android:attr/textAppearanceInverse" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/mainActivityLoginOne"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/loginOne"
        android:textAppearance="?android:attr/textAppearanceInverse" />
</LinearLayout>